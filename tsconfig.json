{
  "compilerOptions": {
    "strict": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "noEmit": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "lib": ["ES2017", "dom"]
  }
}
